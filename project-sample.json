{
  "lang": "en",
  "title": "Frontend Style Guide",
  // if the project is at the host root, 
  // leave only the trailing slash
  "path": "/path_to_project/",
  // these path are relative to project root, 
  // with trailing slash mandatory
  "src_path": "assets/",
  "dest_path": "public/assets/",
  "html_path": "public/",
  "assets_path": "public/assets/",
  "vendor_path": {
    "jquery": {
      "src": "assets/vendor/jquery/dist/jquery.js",
      "dest": "assets/js/vendor/jquery.js"
    },
    "modernizr": {
      "src": "public/assets/vendor/modernizr/modernizr.js",
      "dest": "public/assets/js/vendor/modernizr.js"
    }
  }
}

